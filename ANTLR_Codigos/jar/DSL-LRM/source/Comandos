	rm -v MyDsl__.g
	rm -v MyDslParser.java 
	rm -v MyDslLexer.java  
	rm -v MyDsl.tokens 
	rm -v ../classes/*.class 

	java org.antlr.Tool MyDsl.g
	javac -d ../classes MyDslLexer.java
	javac -d ../classes MyDslParser.java
	javac -d ../classes TLexer.java
	javac -d ../classes ExecutarDsl.java 
	javac -d ../classes Auxiliar.java
	javac -d ../classes Principal.java



	cd ../classes
	java Principal

